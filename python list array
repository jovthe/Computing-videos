#list_name = [item1, item2, item3]


data = [1, 'hello', False, 3, 'new']
print(data)
print(data[1])
print(data[0:3])
print(data[4])
print(data[-1])
print(data[-3])
data[1] = 'hi'
print(data)
data[0:2] = [3, 'bye']
print(data)
data[0:2] = [4]
print(data)
data[0:2] = [3, 5, 'yes']
print(data)
data.append('item3')
print(data)
data.insert(2, 'item4')
print(data)
data.remove(3)
print(data)
data.pop()
print(data)
data.pop(1)
print(data)
del data[2]
print(data)
del data
print(data)
